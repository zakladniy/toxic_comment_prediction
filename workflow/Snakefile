rule all:
    input:
        "data/raw/data_raw.txt",
        "data/interim/data_interim.csv",

rule process_data:
    input:
        "data/raw/data_raw.txt"
    output:
        "data/interim/data_interim.csv"
    shell:
        "python -m src.data.process_data {output}"

rule clean_data:
    input:
        "data/interim/data_interim.csv"
    output:
        "data/processed/data_clean.csv"
    shell:
        "python -m src.data.clean_data {input} {output}"
